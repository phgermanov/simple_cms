<div class="jumbotron pages edit">
<%= link_to("<< Back to List", pages_path, :class => 'back-link btn btn-primary') %>
  <h2>Update Page</h2>

  <%= form_for(@page) do |f| %>

    <%= render(:partial => 'form', :locals => {:f => f}) %>

    <div class="form-buttons">
      <%= f.submit("Update Page", :class => 'btn btn-primary') %>
    </div>

  <% end %>

</div>
